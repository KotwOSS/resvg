<svg 
    xmlns="http://www.w3.org/2000/svg" 
    xmlns:re="http://oss.kotw.dev/resvg"
    xmlns:greet="greet"
    width="100" height="100"
>
    <re:path fill="white" stroke="black" re:translate="20;20">
        <re:moveto x="0" y="0" />
        <re:lineto x="50" y="0" />
        <re:lineto x="50" y="50" />
        <re:lineto x="0" y="50" />
        <re:close />
    </re:path>
</svg>