<svg 
    xmlns="http://www.w3.org/2000/svg" 
    xmlns:re="http://oss.kotw.dev/resvg" 
    width="110" height="110"
>
    <re:repeat x="xrange(0, 1.0, 0.1)">
        <re:repeat y="10">
            <rect
                re:x="x*10" 
                re:y="y*10" 
                width="10"
                height="10"
                fill="rgba({(x+y)/20*255}, 0, 0)"
            />
        </re:repeat>
    </re:repeat>
</svg>